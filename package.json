{
  "name": "gh_scanner",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:IgorByl/gh_scanner.git",
  "author": "Ihar Bulaty <ihar.bulaty@gmail.com>",
  "license": "",
  "scripts": {
    "start": "export SLS_DEBUG=* && sls offline start --reloadHandler --verbose --noAuth --ignoreJWTSignature --noTimeout --stage=local",
    "check-types": "tsc --noEmit",
    "clean": "rimraf .esbuild",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "postinstall": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
    "precommit": "yarn check-types && npx lint-staged --no-stash",
    "tal": "yarn check-types && yarn lint && yarn test"
  },
  "dependencies": {
    "@graphql-tools/schema": "10.0.3",
    "@graphql-tools/utils": "10.2.0",
    "apollo-server-express": "3.13.0",
    "apollo-server-lambda": "3.13.0",
    "graphql": "16.8.1",
    "graphql-constraint-directive": "5.4.3",
    "queue": "7.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.7.10",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.22",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "rimraf": "5.0.7",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.52.1",
    "serverless-offline": "13.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "eslint --cache"
    ]
  }
}
